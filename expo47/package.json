{
  "name": "freecbt",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "yarn _version && expo start",
    "android": "yarn _version && expo start --android",
    "ios": "yarn _version && expo start --ios",
    "web": "yarn _version && expo start --web",
    "lint": "tsc",
    "build": "yarn _version && eas build -p all",
    "build:apk": "yarn _version && eas build -p android --profile apk",
    "build:android": "yarn _version && eas build -p android",
    "build:ios": "yarn _version && eas build -p ios",
    "_version": "./scripts/version.sh > .version.json"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "expo": "~47.0.6",
    "expo-haptics": "^12.0.1",
    "expo-linking": "^3.2.3",
    "expo-localization": "^14.0.0",
    "expo-status-bar": "~1.4.2",
    "i18n-js": "^4.1.1",
    "react": "18.1.0",
    "react-native": "0.70.5",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "^18.0.25",
    "@types/react-native": "^0.70.6",
    "typescript": "^4.9.3"
  },
  "private": true
}
